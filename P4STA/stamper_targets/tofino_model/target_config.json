{
   "target":"tofino_model",
   "cfg_template":"tofino_model_cfg_template.json",
   "target_driver":"../Wedge100B65/tofino1_65p_stamper_v1_0_1.py",
   "stamping_capabilities":{
      "tcp":true,
      "udp":true,
      "timestamp-multi":1,
      "timestamp-max":281474976710655
   },
   "config":{
      "stamper_specific":[
         {
            "title":"SDE",
            "type":"input",
            "target_key":"sde",
            "description":"Path of SDE directory, e.g. /opt/bf-sde-.."
         }
      ]
   },
   "inputs":{
      "input_individual":[
         {
            "title":"Name of P4 program",
            "type":"input",
            "target_key":"program",
            "description":"Name of the P4 program you want to use."
         },
         {
            "title":"SDE",
            "type":"input",
            "target_key":"sde",
            "description":"Path of SDE directory, e.g. /opt/bf.."
         }
      ],
      "input_table":[
         {
            "title":"Link Speed",
            "type":"drop-down",
            "values":[
               "10G",
               "25G",
               "40G",
               "50G",
               "100G"
            ],
            "default_value":"100G",
            "target_key":"speed",
            "description":"Link speed of interface"
         },
         {
            "title":"FEC",
            "type":"drop-down",
            "values":[
               "NONE",
               "FC",
               "RS"
            ],
            "default_value":"NONE",
            "target_key":"fec",
            "description":"Value must be NONE|FC|RS"
         },
         {
            "title":"AN",
            "type":"drop-down",
            "values":[
               "default",
               "on",
               "off"
            ],
            "default_value":"default",
            "target_key":"an",
            "description":"Autonegotiation value must be default|on|off"
         },
         {
            "title":"Limit [Mbit/s]",
            "type":"input",
            "target_key":"shape",
            "description":"Set a throughput limit (outgoing from stamper) in Mbit/s (Traffic Shaping), disable = 0"
         },
         {
            "restrict":"dut",
            "title":"DP Duplication",
            "default_value":"0",
            "type":"input",
            "target_key":"dataplane_duplication",
            "description":"For each packet N additional duplicates egressing this DUT port"
         }
      ]
   },
   "status_check":{
      "needed_sudos_to_add":[
